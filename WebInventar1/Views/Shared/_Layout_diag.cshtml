<body>
    <div class="navbar navbar-default" role="navigation">

        <div class="navbar-inner">
            <a class="navbar-brand" href="index.html">
                <img alt="" src="img/logo20.png" class="hidden-xs" />
                <span>WebInventar</span>
            </a>

            <ul class="collapse navbar-collapse nav navbar-nav top-menu">
                <li><a href="#"> Отчеты</a></li>
            </ul>

        </div>
    </div>
    <!-- topbar ends -->
    <div class="ch-container">
        <div class="row">

            <!-- left menu starts -->
            <div class="col-sm-2 col-lg-2">
                <div class="sidebar-nav">
                    <div class="nav-canvas">
                        <div class="nav-sm nav nav-stacked">

                        </div>
                        <ul class="nav nav-pills nav-stacked main-menu">
                            <li class="nav-header">Техническое обслуживание</li>
                            <li>
                                @Html.ActionLink("Компьютер/Ноутбук", "Diagnostic_comp", new { @class = "ajax-link" })
                            </li>
                            <li>
                                @Html.ActionLink("Принтер/Мфу", "Diagnostic_print", new { @class = "ajax-link" })
                            </li>
                            <li>
                                @Html.ActionLink("Сервер", "Diagnostic_server", new { @class = "ajax-link" })
                            </li>
                            <li class="nav-header">Резервное копирование/восстановление</li>
                            <li>
                                @Html.ActionLink("Копирование/восстановление", "Backup", new { @class = "ajax-link" })
                            </li>
                            <li class="nav-header">Ремонт</li>
                            <li>
                                @Html.ActionLink("Компьютер/Ноутбук", "Repair", new { @class = "ajax-link" })
                            </li>
                            <li>
                                @Html.ActionLink("Принтер/Мфу", "Diagnostic_print", new { @class = "ajax-link" })
                            </li>
                            <li>
                                @Html.ActionLink("Сервер", "Diagnostic_server", new { @class = "ajax-link" })
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!--/span-->
            <!-- left menu ends -->

            @RenderBody()

        </div>

    </div><!--/.fluid-container-->   
    
